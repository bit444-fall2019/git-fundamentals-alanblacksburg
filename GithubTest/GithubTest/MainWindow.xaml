<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GithubTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="OkButton" Content="OK" HorizontalAlignment="Left" Margin="134,279,0,0" VerticalAlignment="Top" Width="70"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="456,279,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.474,-0.402" Width="84"/>
        <Label Content="Your favorite language:" HorizontalAlignment="Left" Margin="41,126,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="LanguageTextbox" HorizontalAlignment="Left" Margin="269,130,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="345" AutomationProperties.HelpText="Enter a programming language name here" ToolTip="Enter a language name here"/>
        <Menu Margin="0,0,0,399">
            <MenuItem Header="&amp;File" Click="MenuItem_Click"/>
        </Menu>

    </Grid>
</Window>
